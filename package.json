{
  "name": "my-vue3-vite-admin",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build:test": "vue-tsc && vite build --mode test",
    "build:prod": "vue-tsc && vite build --mode production",
    "report": "http-server ./dist -o stats.html",
    "deploy": "sh deploy.sh",
    "new": "plop",
    "lint": "npm-run-all -s lint:tsc lint:eslint lint:stylelint",
    "lint:tsc": "vue-tsc",
    "lint:eslint": "eslint . --cache --fix",
    "lint:stylelint": "stylelint \"src/**/*.{css,scss,vue}\" --cache --fix",
    "postinstall": "simple-git-hooks",
    "preinstall": "npx only-allow pnpm",
    "commit": "git pull && git add -A && git cz && git push"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.22",
    "@imengyu/vue3-context-menu": "^1.4.2",
    "@unocss/preset-mini": "^0.62.3",
    "@vueuse/core": "^11.0.3",
    "@vueuse/integrations": "^11.0.3",
    "axios": "^1.7.5",
    "colord": "^2.9.3",
    "defu": "^6.1.4",
    "element-plus": "^2.8.1",
    "floating-vue": "^5.2.2",
    "hotkeys-js": "^3.13.7",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "overlayscrollbars": "^2.10.0",
    "overlayscrollbars-vue": "^0.5.9",
    "path-browserify": "^1.0.1",
    "path-to-regexp": "^7.1.0",
    "pinia": "^2.2.2",
    "qs": "^6.13.0",
    "vue": "^3.4.37",
    "vue-m-message": "^4.0.2",
    "vue-router": "4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.3",
    "@element-plus/icons-vue": "^2.3.1",
    "@iconify/vue": "^4.1.2",
    "@stylistic/stylelint-config": "^2.0.0",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.5.1",
    "@types/nprogress": "^0.2.3",
    "@types/path-browserify": "^1.0.3",
    "@types/qs": "^6.9.15",
    "@types/sortablejs": "^1.15.8",
    "@unocss/core": "^0.62.3",
    "@unocss/eslint-plugin": "^0.62.3",
    "@vitejs/plugin-legacy": "^5.4.2",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "autoprefixer": "^10.4.20",
    "crypto-js": "^4.2.0",
    "cz-git": "^1.9.4",
    "eslint": "^9.9.1",
    "lodash-es": "^4.17.21",
    "plop": "^4.0.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.77.8",
    "simple-git-hooks": "^2.11.1",
    "sortablejs": "^1.15.2",
    "stylelint": "^16.9.0",
    "stylelint-config-recess-order": "^5.1.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-scss": "^6.5.1",
    "typescript": "^5.5.3",
    "unocss": "^0.62.3",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-turbo-console": "^1.10.1",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.1",
    "vite-plugin-compression2": "^1.2.0",
    "vite-plugin-pages": "^0.32.3",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "^7.3.9",
    "vite-plugin-vue-meta-layouts": "^0.4.3",
    "vue-tsc": "^2.0.29"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "preserveUnused": true
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
